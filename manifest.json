{
    "manifest_version": 3,

    "name": "SQL Format",
    "description": "A chrome plugin to format your SQL code",
    "version": "3.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],

    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    }
}
